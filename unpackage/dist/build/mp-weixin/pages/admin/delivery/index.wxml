<view class="deliver-goods"><view class="_header"><view class="order-num acea-row row-between-wrapper"><view class="num line1">{{"订单号："+order_id}}</view><view class="name line1"><label class="iconfont icon-yonghu2 _span"></label>{{delivery.nickname+''}}</view></view><view class="address"><view class="name">{{''+delivery.real_name}}<label class="phone _span">{{delivery.user_phone}}</label></view><view>{{delivery.user_address}}</view></view><view class="line"><image src="/static/images/line.jpg"></image></view></view><view class="wrapper"><view class="item acea-row row-between-wrapper"><view>发货方式</view><view class="mode acea-row row-middle row-right"><block wx:for="{{types}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeType',['$0',index],[[['types','',index]]]]]]]}}" class="{{['goods',active===index?'on':'']}}" bindtap="__e">{{''+item.title}}<label class="iconfont icon-xuanzhong2 _span"></label></view></block></view></view><block wx:if="{{logistics.length>0}}"><block><view hidden="{{!(active===0)}}" class="list"><block wx:if="{{delivery.config_export_open==1}}"><view class="item acea-row row-between-wrapper"><view>发货类型</view><view class="mode acea-row row-middle row-right"><block wx:for="{{expressType}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeExpTpe',['$0',index],[[['expressType','',index]]]]]]]}}" class="{{['goods',curExpress===item.key?'on':'']}}" bindtap="__e">{{''+item.title}}<label class="iconfont icon-xuanzhong2 _span"></label></view></block></view></view></block><block wx:if="{{curExpress==1}}"><block><view class="item acea-row row-between-wrapper"><view>快递公司</view><view class="select-box"><picker class="pickerBox" value="{{seIndex}}" range="{{logistics}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{logistics[seIndex].name}}</view></picker></view></view><view class="item acea-row row-between-wrapper"><view>快递单号</view><input class="mode" type="text" placeholder="填写快递单号" data-event-opts="{{[['input',[['__set_model',['','delivery_id','$event',[]]]]]]}}" value="{{delivery_id}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="iconfont icon-xiangji" bindtap="__e"></text><block wx:if="{{curExpress==1}}"><text class="trip">顺丰请输入单号 :收件人或寄件人手机号后四位</text></block><block wx:if="{{curExpress==1}}"><text class="trip">例如：SF000000000000:3941</text></block></view></block></block><block wx:if="{{curExpress==2}}"><block><view class="item acea-row row-between-wrapper"><view>快递公司</view><view class="select-box"><picker class="pickerBox" value="{{seIndex}}" range="{{logistics}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{logistics[seIndex].name}}</view></picker></view></view><block wx:if="{{expTemp.length>0}}"><view class="item acea-row row-between-wrapper"><view>电子面单</view><view style="display:flex;align-items:center;" class="_div"><picker class="pickerBox" value="{{expIndex}}" range="{{expTemp}}" range-key="title" data-event-opts="{{[['change',[['bindTempChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{expTemp[expIndex].title}}</view></picker><view data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" class="look _div" bindtap="__e">预览</view></view></view></block><view class="item acea-row row-between-wrapper"><view>寄件人姓名：</view><input class="mode" type="text" placeholder="填写寄件人姓名" data-event-opts="{{[['input',[['__set_model',['','to_name','$event',[]]]]]]}}" value="{{to_name}}" bindinput="__e"/></view><view class="item acea-row row-between-wrapper"><view>寄件人电话：</view><input class="mode" type="text" placeholder="填写寄件人电话" data-event-opts="{{[['input',[['__set_model',['','to_tel','$event',[]]]]]]}}" value="{{to_tel}}" bindinput="__e"/></view><view class="item acea-row row-between-wrapper"><view>寄件人地址：</view><input class="mode" type="text" placeholder="填写寄件人地址" data-event-opts="{{[['input',[['__set_model',['','to_addr','$event',[]]]]]]}}" value="{{to_addr}}" bindinput="__e"/></view></block></block></view></block></block><view hidden="{{!(active===1)}}" class="list"><view class="item acea-row row-between-wrapper"><view>送货人</view><block wx:if="{{postPeople.length>0}}"><view class="select-box"><picker class="pickerBox" value="{{postIndex}}" range="{{postPeople}}" range-key="wx_name" data-event-opts="{{[['change',[['bindPostChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{postPeople[postIndex].wx_name}}</view></picker></view></block></view></view><textarea hidden="{{!(active===2)}}" class="textarea" placeholder="备注" maxlength="{{500}}" auto-height="{{true}}" data-event-opts="{{[['blur',[['bindTextAreaBlur',['$event']]]],['input',[['__set_model',['','fictitious_content','$event',[]]]]]]}}" value="{{fictitious_content}}" bindblur="__e" bindinput="__e"></textarea></view><view style="height:1.2rem;"></view><view data-event-opts="{{[['tap',[['saveInfo',['$event']]]]]}}" class="confirm" bindtap="__e">确认提交</view></view>