.w-time-picker.show{-webkit-transform:translateZ(0);transform:translateZ(0)}.w-time-picker{position:fixed;left:0;top:0;width:100%;height:100%;z-index:9999;background-color:#f5f5f5;display:flex;flex-direction:column;-webkit-transform:translate3d(100%,0,0);transform:translate3d(100%,0,0);transition:all .3s ease}.w-time-picker .w-time-days{overflow:hidden;padding:20rpx 0;background-color:#fff}.w-time-picker .w-time-days .w-time-scroll{white-space:nowrap}.w-time-picker .w-time-days .w-time-day{display:inline-block;width:120rpx;text-align:center;border:solid 1px #ddd;margin:0 20rpx;border-radius:6rpx;padding:10rpx 0;color:#333}.w-time-picker .w-time-days .w-time-day .w-time-week{font-size:28rpx;line-height:1}.w-time-picker .w-time-days .w-time-day .w-time-date{font-size:24rpx;line-height:1;margin-top:10rpx}.w-time-picker .w-time-days .w-time-day-active{color:red;border-color:red}.w-time-picker .w-time-body{flex:1;overflow:hidden}.w-time-picker .w-time-body .w-time-list-scroll{height:100%}.w-time-picker .w-time-body .w-time-list{display:flex;flex-wrap:wrap;padding:20rpx 10rpx}.w-time-picker .w-time-body .w-time-item{width:120rpx;height:64rpx;line-height:64rpx;margin:0 10rpx 20rpx;text-align:center;border:solid 1px #ddd;border-radius:6rpx;font-size:28rpx;transition:all .3s ease}.w-time-picker .w-time-body .w-time-item-active{background-color:#fff}.w-time-picker .w-time-footer{height:88rpx;display:flex;background-color:#fff}.w-time-picker .w-time-footer .w-time-btn{flex:1;text-align:center;line-height:88rpx;font-size:30rpx}.w-time-picker .w-time-footer .w-time-sure{background-color:red;color:#fff}