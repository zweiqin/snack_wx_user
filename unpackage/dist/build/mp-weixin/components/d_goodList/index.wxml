<view class="goodsList"><block wx:for="{{tempArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['tempArr','',index]]]]]]]}}" class="item" bindtap="__e"><view class="pictrue"><block wx:if="{{item.activity&&item.activity.type==='1'}}"><label class="pictrue_log pictrue_log_class _span">秒杀</label></block><block wx:if="{{item.activity&&item.activity.type==='2'}}"><label class="pictrue_log pictrue_log_class _span">砍价</label></block><block wx:if="{{item.activity&&item.activity.type==='3'}}"><label class="pictrue_log pictrue_log_class _span">拼团</label></block><block wx:if="{{item.recommend_image}}"><image src="{{item.recommend_image}}" mode></image></block><block wx:else><image src="{{item.image}}" mode></image></block></view><view class="text line2">{{item.store_name}}</view><view class="bottom acea-row row-between-wrapper"><view class="sales acea-row row-middle"><view class="money font-color"><text>￥</text>{{item.price}}</view><view>{{"已售 "+item.sales}}</view></view><block wx:if="{{item.stock>0}}"><view><block wx:if="{{item.activity&&(item.activity.type==='1'||item.activity.type==='2'||item.activity.type==='3')||item.is_virtual}}"><view class="bnt">立即购买</view></block><block wx:else><view><block wx:if="{{item.spec_type}}"><view data-event-opts="{{[['tap',[['goCartDuo',['$0'],[[['tempArr','',index]]]]]]]}}" class="bnt" catchtap="__e">加入购物车<block wx:if="{{isLogin&&item.cart_num}}"><view class="num">{{item.cart_num}}</view></block></view></block><block wx:if="{{!item.spec_type&&!item.cart_num}}"><view data-event-opts="{{[['tap',[['goCartDan',['$0',index],[[['tempArr','',index]]]]]]]}}" class="bnt" catchtap="__e">加入购物车</view></block><block wx:if="{{!item.spec_type&&item.cart_num}}"><view class="cart acea-row row-middle"><view data-event-opts="{{[['tap',[['CartNumDes',[index,'$0'],[[['tempArr','',index]]]]]]]}}" class="pictrue iconfont icon-jianhao" catchtap="__e"></view><view class="num">{{item.cart_num}}</view><view data-event-opts="{{[['tap',[['CartNumAdd',[index,'$0'],[[['tempArr','',index]]]]]]]}}" class="pictrue iconfont icon-jiahao" catchtap="__e"></view></view></block></view></block></view></block><block wx:else><view class="bnt end">已售罄</view></block></view></view></block></view>