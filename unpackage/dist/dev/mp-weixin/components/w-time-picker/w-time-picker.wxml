<view class="{{['w-time-picker',(showTimePicker)?'show':'']}}"><view class="w-time-body"><scroll-view class="w-time-list-scroll" scroll-y="{{true}}"><view class="w-time-list"><block wx:for="{{timeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toggleItem',['$0',index],[[['timeList','',index]]]]]]]}}" class="{{['w-time-item',(!item.disabled)?'w-time-item-active':'']}}" style="{{'color:'+(itemIndex==index?theme:'#333')+';'+('border-color:'+(itemIndex==index?theme:'#ddd')+';')}}" bindtap="__e">{{item.label}}</view></block></view></scroll-view></view><view class="w-time-footer"><view data-event-opts="{{[['tap',[['hide',['$event']]]]]}}" class="w-time-cancel w-time-btn" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="w-time-sure w-time-btn" style="{{'background-color:'+(theme)+';'}}" bindtap="__e">确定</view></view></view>