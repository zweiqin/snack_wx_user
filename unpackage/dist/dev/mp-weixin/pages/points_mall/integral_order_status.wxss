.coupons .title {
  margin: 30rpx 0 25rpx 0;
}
.coupons .title .line {
  width: 70rpx;
  height: 1px;
  background: #DCDCDC;
}
.coupons .title .name {
  font-size: 24rpx;
  color: #999;
  margin: 0 10rpx;
}
.coupons .list {
  padding: 0 20rpx;
}
.coupons .list .item {
  margin-bottom: 20rpx;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.06);
}
.coupons .list .item .price {
  width: 236rpx;
  height: 160rpx;
  font-size: 26rpx;
  color: #fff;
  font-weight: 800;
}
.coupons .list .item .price text {
  font-size: 54rpx;
}
.coupons .list .item .text {
  width: 385rpx;
}
.coupons .list .item .text .name {
  font-size: #282828;
  font-size: 30rpx;
}
.coupons .list .item .text .priceMin {
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}
.coupons .list .item .text .time {
  font-size: 24rpx;
  color: #999;
  margin-top: 15rpx;
}
.coupons .list .open {
  font-size: 24rpx;
  color: #999;
  margin-top: 30rpx;
}
.coupons .list .open .iconfont {
  font-size: 25rpx;
  margin: 5rpx 0 0 10rpx;
}
.payment-status {
  background-color: #fff;
  margin: 195rpx 30rpx 0 30rpx;
  border-radius: 10rpx;
  padding: 1rpx 0 28rpx 0;
}
.payment-status .icons {
  font-size: 70rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  color: #fff;
  text-align: center;
  line-height: 140rpx;
  text-shadow: 0px 4px 0px rgba(255, 255, 255, 0.5);
  border: 6rpx solid #f5f5f5;
  margin: -76rpx auto 0 auto;
  background-color: #999;
}
.payment-status .icons.icon-iconfontguanbi {
  text-shadow: 0px 4px 0px #6c6d6d;
}
.payment-status .iconfont.fail {
  text-shadow: 0px 4px 0px #7a7a7a;
}
.payment-status .status {
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  margin: 25rpx 0 37rpx 0;
}
.payment-status .wrapper {
  border: 1rpx solid #eee;
  margin: 0 30rpx 47rpx 30rpx;
  padding: 35rpx 0;
  border-left: 0;
  border-right: 0;
}
.payment-status .wrapper .item {
  font-size: 28rpx;
  color: #282828;
}
.payment-status .wrapper .item ~ .item {
  margin-top: 20rpx;
}
.payment-status .wrapper .item .itemCom {
  color: #666;
}
.payment-status .returnBnt {
  width: 630rpx;
  height: 86rpx;
  border-radius: 50rpx;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 86rpx;
  margin: 0 auto 20rpx auto;
}

