.default {
  width: 690rpx;
  height: 300rpx;
  border-radius: 14rpx;
  margin: 26rpx auto 0 auto;
  background-color: #ccc;
  text-align: center;
  line-height: 300rpx;
}
.default .iconfont {
  font-size: 50rpx;
}
.title .text {
  display: flex;
  align-items: flex-end;
}
.title .text .txt-btn {
  margin-bottom: 6rpx;
  margin-left: 6px;
}
.coupon {
  margin: 0 30rpx;
  padding: 0rpx 0 32rpx 0;
  background-color: #fff;
  border-radius: 12rpx;
}
.coupon .conter {
  margin-top: 25rpx;
  padding: 0 20rpx;
}
.coupon .conter .itemCon {
  background-size: 100% 100%;
  width: 226rpx;
  height: 108rpx;
  display: inline-block;
  margin-right: 14rpx;
}
.coupon .conter .itemCon .on .item {
  background-color: #C5C5C5 !important;
}
.coupon .conter .itemCon .on .icon-youhuiquantoumingbeijing {
  color: #f3f3f3 !important;
}
.coupon .conter .itemCon .on {
  background-color: #C5C5C5;
}
.coupon .conter .itemCon .on .icon-youhuiquantoumingbeijing {
  color: #f3f3f3 !important;
}
.coupon .conter .itemCon .on .man {
  background-color: #AEAEAE;
}
.coupon .conter .itemCon .on .bnt {
  color: #CCCCCC;
}
.coupon .conter .itemCon .no {
  background: linear-gradient(135deg, var(--view-main-start) 0%, var(--view-main-over) 100%);
}
.coupon .conter .itemCon .no .icon-youhuiquantoumingbeijing {
  color: #F8E8E6;
}
.coupon .conter .itemCon .no .man {
  background-color: var(--view-theme);
}
.coupon .conter .itemCon .no .bnt {
  color: var(--view-priceColor);
}
.coupon .conter .itemCon .item {
  width: 100%;
  height: 100%;
  position: relative;
  border-radius: 10rpx;
}
.coupon .conter .itemCon .item .cir {
  position: absolute;
  left: -6rpx;
  top: -6rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #fff;
  z-index: 100;
}
.coupon .conter .itemCon .item .cir2 {
  position: absolute;
  left: -6rpx;
  bottom: -6rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #fff;
  z-index: 100;
}
.coupon .conter .itemCon .item .icon-youhuiquantoumingbeijing {
  position: absolute;
  right: 0;
  width: 88rpx;
  height: 100%;
  font-size: 124rpx;
  line-height: 124rpx;
  height: 124rpx;
  z-index: 99;
}
.coupon .conter .itemCon .item .text {
  padding-left: 20rpx;
  color: #fff;
  font-size: 22rpx;
  width: 168rpx;
  padding-bottom: 6rpx;
  z-index: 99;
}
.coupon .conter .itemCon .item .text .money {
  font-size: 30rpx;
  font-weight: bold;
}
.coupon .conter .itemCon .item .text .money .num {
  font-size: 44rpx;
}
.coupon .conter .itemCon .item .text .man {
  width: -webkit-max-content;
  width: max-content;
  border-radius: 20rpx;
  padding: 0 10rpx;
}
.coupon .conter .itemCon .item .bnt {
  -webkit-writing-mode: vertical-lr;
          writing-mode: vertical-lr;
  font-size: 22rpx;
  margin: 0 12rpx;
  z-index: 99;
}

